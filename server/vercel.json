{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "dist/**"
                ]
            }
        }
    ],
    "functions": {
    "api/**/*.js": {
      "includeFiles": [
        "node_modules/@clerk/**"
      ]
    }
  },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ]
}